{
  "flex-layout.row#deals": {
    "children": [
      "rich-text#estilos-alimentacao",
      "slider-layout#deals"
    ],
    "props": {
      "blockClass": "deals"
    }
  },
  "rich-text#estilos-alimentacao": {
    "props": {
      "text": "Estilos de \n alimentação",
      "textAlignment": "left",
      "blockClass": ["deals", "deals--title"],
      "width": "15%"
    }
  },
  "slider-layout#deals": {
    "children": [
      "flex-layout.col#deals1", 
      "flex-layout.col#deals2", 
      "flex-layout.col#deals3", 
      "flex-layout.col#deals4",
      "flex-layout.col#deals5",
      "flex-layout.col#deals6"
    ],
    "props": {
      "showPaginationDots": "never",
      "showNavigationArrows": "mobileOnly",
      "itemsPerPage": {
        "desktop": 6,
        "phone": 3
      },
      "blockClass": "categories-slider"
    }
  },

  "flex-layout.col#deals1": {
    "children": [
      "image#deal1",
      "rich-text#deal1"
    ]
  },
  "image#deal1": {
    "props": {
      "src": "/arquivos/low_carb.png"
    }
  },
  "rich-text#deal1": {
    "props": {
      "text": "[Low Carb](https://www.mundoverde.com.br/dietas-especiais/low-carb/)",
      "blockClass": "deals"
