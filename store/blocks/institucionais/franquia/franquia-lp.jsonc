	//Main banner
	"flex-layout.row#franchise-main-banner": {
		"children":[
      "flex-layout.col#franchise-main-banner",
      "flex-layout.col#franchise-main-banner-text",
      "flex-layout.col#franchise-main-banner-empty"
    ],
    "props": {
      "blockClass": "franchise-main-banner",
      "preventVerticalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "horizontalAlign": "left"
    },
		"title": "Banner principal"		
	},

	//Description
	"flex-layout.row#franchise-description": {
		"children":[
			"rich-text#franchise-description-text",
			"list-context.image-list#franchise-description-image"
		],
		"props":{
      "blockClass": "franchise-description",
      "colSizing": "equal",
      "preserveLayoutOnMobile": true,
      "preventVerticalStretch": true
    },
		"title": "Descrição"
	},

	//Brand
	"flex-layout.row#franchise-brand": {
		"children":[
			"flex-layout.col#franchise-brand-box"
		],
    "props": {
      "blockClass": "franchise-brand",
      "colSizing": "equal",
      "fullWidth": true,
      "preserveLayoutOnMobile": true
    },
		"title":"Marca"
	},

	//Tips
	"flex-layout.row#franchise-tips": {
		"children":["flex-layout.col#franchise-tips"],
    "props": {
      "blockClass": "franchise-tips",
      "colSizing": "equal",
      "fullWidth": true,
      "horizontalAlign": "left",
      "preserveLayoutOnMobile": true
    },
		"title":"Dicas"
	},
	
	//Benefits
	"flex-layout.row#franchise-benefits": {
		"children":["flex-layout.col#franchise-benefits"],
    "props": {
      "blockClass": "franchise-benefits",
      "colSizing": "equal",
      "fullWidth": true,
      "preserveLayoutOnMobile": true
    },
		"title":"Benefícios"
	},

	//Selection 1
	"flex-layout.row#franchise-selection-1": {
		"children": ["flex-layout.col#franchise-selection-content"],
    "props": {
      "blockClass": ["franchise-selection", "franchise-selection-first"],
      "colSizing": "equal",
      "fullWidth": true,
      "horizontalAlign":"center",
      "preserveLayoutOnMobile": true
    },
		"title":"Seleção - Parte 1"
	},

	//Selection 2
	"flex-layout.row#franchise-selection-2": {
		"children": [
			"flex-layout.col#franchise-selection-col3",
			"flex-layout.col#franchise-selection-col4"
		],
