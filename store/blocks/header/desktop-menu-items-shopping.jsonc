      "menu-item#shopping-all-departments",
      "menu-item#menu-item-alimentos",
      "menu-item#menu-item-suplementos",
      "menu-item#menu-item-outros",
      "menu-item#menu-item-marcas",
      "menu-item#menu-item-ofertas"
    ],
    "props": {
      "blockClass": "header-categoryMenu"
    },
    "title": "Menu Horizontal"
  },

  "submenu#shopping-submenu-desktop": {
    "children": [
      "menu#shopping-menu-desktop",
